            ]
          },
          {
            "group": "Pet",
            "openapi": "api-reference/petstore.json",
            "pages": [
              "POST /pet",
              "PUT /pet",
              "GET /pet/findByStatus",
              "GET /pet/findByTags",
              "GET /pet/{petId}",
              "POST /pet/{petId}",
              "DELETE /pet/{petId}",
              "POST /pet/{petId}/uploadImage"
            ]
          },
          {
            "group": "Store",
            "openapi": "api-reference/petstore.json",
            "pages": [
              "GET /store/inventory",
              "POST /store/order",
              "GET /store/order/{orderId}",
              "DELETE /store/order/{orderId}"
            ]
          },
          {
            "group": "User",
            "openapi": "api-reference/petstore.json",
            "pages": [
              "POST /user",
              "POST /user/createWithArray",
              "POST /user/createWithList",
              "GET /user/login",
              "GET /user/logout",
              "GET /user/{username}",
              "PUT /user/{username}",
              "DELETE /user/{username}"
            ]
          }
        ]
      },
      {
        "tab": "Instagram API",
        "groups": [
          {
            "group": "Users",
            "openapi": "api-reference/instagram.yaml",
            "pages": [
              "GET /users/search",
              "GET /users/self/feed",
              "GET /users/self/media/liked",
              "GET /users/{user-id}",
              "GET /users/{user-id}/media/recent"
            ]
          },
          {
            "group": "Relationships",
            "openapi": "api-reference/instagram.yaml",
            "pages": [
              "GET /users/self/requested-by",
              "GET /users/{user-id}/followed-by",
              "GET /users/{user-id}/follows",
              "GET /users/{user-id}/relationship",
              "POST /users/{user-id}/relationship"
            ]
          },
          {
            "group": "Media",
            "openapi": "api-reference/instagram.yaml",
            "pages": [
              "GET /media/popular",
              "GET /media/search",
              "GET /media/shortcode/{shortcode}",
              "GET /media/{media-id}"
            ]
          },
