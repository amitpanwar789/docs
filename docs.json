              "GET /tags/{tag-name}",
              "GET /tags/{tag-name}/media/recent"
            ]
          },
          {
            "group": "Locations",
            "openapi": "api-reference/instagram.yaml",
            "pages": [
              "GET /locations/search",
              "GET /locations/{location-id}",
              "GET /locations/{location-id}/media/recent"
            ]
          },
          {
            "group": "Geographies",
            "openapi": "api-reference/instagram.yaml",
            "pages": [
              "GET /geographies/{geo-id}/media/recent"
            ]
          }
